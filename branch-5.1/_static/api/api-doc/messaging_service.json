   "models":{
      "message_counter":{
         "id":"message_counter",
         "description":"Holds command counters",
         "properties":{
            "value":{
               "type":"long"
            },
            "key":{
               "type":"string"
            }
         }
      },
      "verb_counter":{
         "id":"verb_counters",
         "description":"Holds verb counters",
         "properties":{
            "count":{
               "type":"long"
            },
            "verb":{
               "type":"string",
               "enum":[
                 "CLIENT_ID",
                 "MUTATION",
                 "MUTATION_DONE",
                 "READ_DATA",
                 "READ_MUTATION_DATA",
                 "READ_DIGEST",
                 "GOSSIP_ECHO",
                 "GOSSIP_DIGEST_SYN",
                 "GOSSIP_DIGEST_ACK2",
                 "GOSSIP_SHUTDOWN",
                 "DEFINITIONS_UPDATE",
                 "TRUNCATE",
                 "REPLICATION_FINISHED",
                 "MIGRATION_REQUEST",
                 "PREPARE_MESSAGE",
                 "PREPARE_DONE_MESSAGE",
                 "UNUSED__STREAM_MUTATION",
                 "STREAM_MUTATION_DONE",
                 "COMPLETE_MESSAGE",
                 "UNUSED__REPAIR_CHECKSUM_RANGE",
                 "GET_SCHEMA_VERSION"
               ]
            }
         }
      }
