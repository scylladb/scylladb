         "operations":[
            {
               "method":"GET",
               "summary":"Get the version number",
               "type": "long",
               "nickname":"get_version",
               "produces":[
                  "application/json"
               ],
               "parameters":[
                  {
                     "name":"addr",
                     "description":"Address",
                     "required":true,
                     "allowMultiple":false,
                     "type":"string",
                     "paramType":"query"
                  }
               ]
            }
